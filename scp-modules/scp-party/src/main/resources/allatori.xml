<config>
    <!-- scp-xxx-compile 原始jar文件，scp-xxx 混淆后的输出的jar文件 -->
    <input>
        <jar in="../../../target/scp-party-compile.jar" out="../../../target/scp-party.jar"/>
    </input>
    <!-- 不替换类名、方法名、属性名 -->
    <keep-names>
        <class access="protected+">
            <field access="private+"/>
            <method access="protected+"/>
        </class>
    </keep-names>
    <!-- 接口形参名保持不变 -->
    <property name="local-variables-naming" value="keep-parameters"/>
    <!-- 排除-->
    <ignore-classes>
        <!-- 排除指定路径 -->
        <class template="class  com.bjzhianjia.scp.party.config.*"/>
        <class template="class  com.bjzhianjia.scp.party.mapper.*"/>
        <class template="class  com.bjzhianjia.scp.party.feign.*"/>
        <class template="class  com.bjzhianjia.scp.party.entity.*"/>
        <class template="class  com.bjzhianjia.scp.party.util.*"/>
        <class template="class  com.bjzhianjia.scp.party.vo.*"/>
        <class template="class  com.bjzhianjia.scp.party.constances.*"/>
        <class template="class  com.bjzhianjia.scp.party.exception.*"/>
        <class template="class  com.bjzhianjia.scp.party.PartyBootstrap"/>
        <!-- 排除springboot 依赖文件 -->
        <class template="class  org.springframework.boot.*"/>
    </ignore-classes>
    <!-- 随机命名混淆字符-->
    <property name="random-seed" value="cshxzf jrncbd hh"/>
</config>
